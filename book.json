{
  "title": "小陈唠唠嗑",
  "author": "lazy",
  "description": "select * from learn",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "styles/website.css",
    "ebook": "styles/ebook.css",
    "pdf": "styles/pdf.css",
    "mobi": "styles/mobi.css",
    "epub": "styles/epub.css"
  },
  "links": {
    "sidebar": {}
  },
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "-sharing",
    "-lunr",
    "-search",
    "search-plus",
    "splitter",
    "expandable-chapters-small",
    "tbfed-pagefooter",
    "anchor-navigation-ex",
    "copy-code-button",
    "theme-comscore",
    "hide-element"
  ],
  "pluginsConfig": {
    "theme-api": {
      "theme": "dark"
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy lazy 2021",
      "modify_label": "这个时间段你改了文件诶：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "theme-default": {
      "showLevel": true
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    }
  }
}